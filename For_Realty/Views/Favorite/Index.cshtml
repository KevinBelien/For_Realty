@model IEnumerable<For_Realty.Models.Favorite>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.RealEstate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UserAccount)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.RealEstate.Description)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UserAccount.Givenname)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.FavoriteID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.FavoriteID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.FavoriteID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
