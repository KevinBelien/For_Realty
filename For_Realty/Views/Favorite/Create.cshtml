@model For_Realty.Models.Favorite

@{
    ViewData["Title"] = "Create";
}

<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="RealEstateID" class="control-label"></label>
                <select asp-for="RealEstateID" class ="form-control" asp-items="ViewBag.RealEstateID"></select>
            </div>
            <div class="form-group">
                <label asp-for="UserAccountID" class="control-label"></label>
                <select asp-for="UserAccountID" class ="form-control" asp-items="ViewBag.UserAccountID"></select>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>


@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
